   "source": [
    "# Principal Component Analysis\n",
    "\n",
    "import numpy as np\n",
    "dataset = np.array([[2, 4, 6], [1,3,5]])\n",
    "mean = np.mean(dataset, axis=1, keepdims=True)\n",
    "X_centered = dataset - mean\n",
    "cov = np.cov(X_centered)\n",
    "eigen_val, eigen_vector = np.linalg.eig(cov)\n",
    "print(eigen_val, eigen_vector)\n",
    "\n"
   ]
  }
 ],
